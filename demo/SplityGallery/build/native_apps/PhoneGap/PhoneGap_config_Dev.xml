<?xml version="1.0" encoding="UTF-8" ?><widget xmlns = "http://www.w3.org/ns/widgets"	xmlns:gap = "http://phonegap.com/ns/1.0"	id        = "fr.intermedia-paris.test1"	version   = "0.0.1">	<name>Test1</name>	<description>Test1</description>	<author>Intermedia Paris</author>		<!-- phonegap release -->	<preference name="phonegap-version" value="2.1.0" />		<preference name="fullscreen" value="true" />		<!-- device orientation -->	<preference name="orientation" value="both" />		<preference name="exit-on-suspend" value="true" />		<!-- app is designed for phone and tablets -->	<preference name="target-device" value="universal" />		<preference name="webviewbounce" value="false" />		<!-- iOS specific to avoid youtube links to open in safari -->	<!-- removed as when clicking on links, the target opens in the app and there is no way to back to the detail view -->	<!-- <preference name="stay-in-webview" value="true" /> -->	<!-- iOS specific to have a black status bar -->	<preference name="ios-statusbarstyle" value="black-opaque" />		<!-- the default icon attribute -->	<preference name="prerendered-icon" value="true" />	<icon src="App_114.png" default="true" />	<!-- iOS icons: classic iPhone screens, Retina displays, and iPad screens -->	<icon src="icons/icon_29.png" width="29" height="29"/>	<icon src="icons/icon_48.png" width="48" height="48"/>	<icon src="icons/icon_57.png" width="57" height="57"/>	<icon src="icons/icon_72.png" width="72" height="72"/>	<icon src="icons/icon_114.png" width="114" height="114"/>	<icon src="icons/icon_512.png" width="512" height="512"/>	<icon src="icons/icon_1024.png" width="1024" height="1024"/>	<!-- Android require specific dimensions for icons: ldpi, mdpi, and hdpi -->	<icon src="icons/icon_36.png" gap:platform="android" gap:density="ldpi" />	<icon src="icons/icon_48.png" gap:platform="android" gap:density="mdpi" />	<icon src="icons/icon_72.png" gap:platform="android" gap:density="hdpi" />	<!-- Default splash screen -->	<gap:splash src="splash.png" />	<!-- iOS requires specific dimensions for different devices: classic iPhone	screens, Retina displays, and iPad screens -->	<gap:splash src="splashScreens/Default.png" width="320" height="480" />	<gap:splash src="splashScreens/Default-Landscape.png" width="1024" height="768" />	<!-- Android requires specific dimensions for different devices: ldpi, mdpi, and hdpi screens -->	<!--<gap:splash src="splashScreens/splashScreen-240x320.png" gap:platform="android" gap:density="ldpi" />	<gap:splash src="splashScreens/splashScreen-320x480.png" gap:platform="android" gap:density="mdpi" />	<gap:splash src="splashScreens/splashScreen-480x800.png" gap:platform="android" gap:density="hdpi" />-->		<!-- If you do not want any permissions to be added to your app, add the    following tag to your config.xml; you will still have the INTERNET    permission on your app, which PhoneGap requires. -->	<preference name="permissions" value="none"/></widget>